package insanity;

import java.util.Date;

public class HighScore {

    private int key;
    private String player;
    private int score;
    private Date date;

    public HighScore(int key, String player, int score, Date date) {
        this.key = key;
    	this.player = player;
        this.score = score;
        this.date = date;
    }

    public HighScore() {
		// TODO Auto-generated constructor stub
	}

	public int getKey() {
        return key;
    }

    public String getPlayer() {
        return player;
    }

    public int getScore() {
        return score;
    }

    public Date getDate() {
        return date;
    }
    
    public void setKey(int key) {
        this.key = key;
    }

    public void setPlayer(String player) {
        this.player = player;
    }

    public void setScore(int score) {
        this.score = score;
    }

    public void setDate(Date date) {
        this.date = date;
    }
}